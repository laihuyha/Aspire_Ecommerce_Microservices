# Environment configuration for development
# Copy this file to .env and adjust values as needed

# ASP.NET Core Environment
ASPNETCORE_ENVIRONMENT=Development

# Database Configuration
POSTGRES_DB=CatalogDB
POSTGRES_USER=postgres
POSTGRES_PASSWORD=devpassword123
POSTGRES_PORT=5433

# Redis Configuration
REDIS_CONNECTION=distributedcache:6379
REDIS_PORT=6379

# Redis Commander (Web UI for Redis)
REDIS_COMMANDER_USER=admin
REDIS_COMMANDER_PASSWORD=devpassword123
REDIS_COMMANDER_PORT=7001

# API Ports
CATALOG_HTTP_PORT=6000
CATALOG_HTTPS_PORT=6060

# Docker Registry (optional)
DOCKER_REGISTRY=catalog
TAG=latest

# Development-specific settings
# These can be adjusted based on your development needs
