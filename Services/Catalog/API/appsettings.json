{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Catalog": "Information"
    }
  },
  "Database": {
    "Username": "catalog_user",
    "Password": "catalog_dev_password",
    "DatabaseName": "catalogdb",
    "Type": "PostgreSQL"
  },
  "Cache": {
    "MaxMemory": "256mb",
    "PersistenceKeys": 200
  },
  "CertificateSetup": {
    "Enabled": true,
    "AutoSetup": true,
    "ForceRegenerate": false
  },
  "CatalogApi": {
    "ExternalHttpPort": 6000,
    "ExternalHttpsPort": 6060,
    "InternalHttpPort": 8080,
    "InternalHttpsPort": 8081
  }
}
